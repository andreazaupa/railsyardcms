<tr id='user_item_<%= item.id %>'>
    <td>
        <%= item.firstname %>
    </td>
    <td>
        <%= item.lastname %>
    </td>
    <td>
        <%= item.email %>
    </td>
    <td calss="center">
        <%= item.enabled ? 'v' : 'x' %>
    </td>
    <td class="center">
        <%= link_to "edit", edit_admin_user_path(item) %>
        <%= link_to "toggle", toggle_admin_user_path(item), :remote => true, :method => :post %>
        <%= link_to "delete", admin_user_path(item), :class => 'icon-delete', :method => :delete, :confirm => t('admin.users.are_you_sure_to_delete', :name => item.email) %>
    </td>
</tr>