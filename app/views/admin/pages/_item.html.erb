<tr id='page-<%= object.id %>'>
	<td width="120px"><span><%= link_to object.name, admin_page_path(object) %></span></td>
  	<td width="150px"><%= l object.updated_at, :format => :date_with_parenthesized_time %></td>
	<td align="center"><%= object.reserved? ? '<div class="icon-lock-bw"><span class="invisible">V</span></div>'.hs : '<div class="icon-unlock-bw"><span class="invisible">V</span></div>'.hs %></td>
	<td align="center"><%= object.published? ? (link_to_remote "<span class='invisible'>v</span>".hs, :url => toggle_admin_page_path(object, :pagination => params[:pagination]), :html => {:class => 'icon-v'}) : (link_to_remote "<span class='invisible'>x</span>".hs, :url => toggle_admin_page_path(object, :pagination => params[:pagination]), :html => {:class => 'icon-x'}) %></td>
	<td align="center"><%= link_to "<span class='invisible'>delete</span>".hs, admin_page_path(object), :class => 'icon-delete', :method => :delete, :confirm => t('admin.pages.are_you_sure_to_delete', :name => object.name) %></td>
</tr>
